        "GET",
        "POST"
      ],
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "auth-api",
          "Port": 8080
        }
      ],
      "DownstreamPathTemplate": "/User"
    },
    {
      "UpstreamPathTemplate": "/User/{id}",
      "UpstreamHttpMethod": [
        "GET",
        "DELETE",
        "PUT"
      ],
      "DownstreamScheme": "http",
