        {
          "Host": "auth-api",
          "Port": 8080
        }
      ],
      "DownstreamPathTemplate": "/User/{id}"
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  }
}
